@article{Williams2010,
  ids = {williamsPeoplePreferentiallyIncrease2010},
  title = {People {{Preferentially Increase Hip Joint Power Generation}} to {{Walk Faster Following Traumatic Brain Injury}}},
  author = {Williams, Gavin and Morris, Meg E. and Schache, Anthony and McCrory, Paul R.},
  year = {2010},
