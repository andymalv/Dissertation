@article{Kelso2014,
  title = {The {{Dynamic Brain}} in {{Action}}: {{Coordinative Structures}}, {{Criticality}}, and {{Coordination Dynamics}}},
  author = {Kelso, J A Scott},
  year = {2014},
  journal = {Criticality in Neural Systems},
  pages = {67--104},
  doi = {10.1002/9783527651009.ch4},
  abstract = {Criticality in Neural Systems\vphantom\{\}, year = \{2014\}, abstract = \{Coordinative structures -- collectives of elements temporarily constrained to accomplish a goal, task, or function -- are proposed as the basis of neural and behavioral function. Coordinative structures are functional linkages, softly assembled, and context sensitive. They have multiple realizations and manifestations, from the cooperative behavior of molecules and cells to the pattern-generating capacities of multifunctional neural circuits within and between brains: they span all kinds of processes at every known level of biological and social organization. They are nature's way to handle degeneracy: combinations of different elements and the recruitment of new ones through alternate paths are used to accomplish the same functional outcome. Although shaped by evolution, development, and learning, experiments and theoretical modeling at both behavioral and neural levels have demonstrated that coordinative structures are self-organized: they exhibit all the predicted features of nonequilibrium phase transitions, a basic form of self-organization in nature. Self-organization refers to the spontaneous formation and change of patterns that occurs near criticality in nature's open systems. Switching and decision making are observed to occur in the brain and behavior, but there are no switches or decision makers. By living near criticality, the brain exploits variability and time-dependent parameter tuning to explore the range of potential options available -- its full dynamical repertoire -- and to select those that fit current circumstances. Although it has proved possible to derive or synthesize the relation within and between behavioral and neural levels of description, even in their simplest form the nonlinear dynamical laws (Coordination Dynamics) appear to be truly emergent and sui generis. They fall into the category of Laughlin and Pines's (2000) ``protectorates'' or ``higher order principles,'' Anderson's (1972) ``more is different,'' and Schr{\"o}dinger's (1944) ``new laws to be expected in the organism'' (Kelso and Haken, 1995; Murphy and O'Neill, 1995). As Einstein was reported to say, you only need to look at living things to see how primitive physics is. Still.\vphantom\}},
  isbn = {9783527651009},
  keywords = {coordination dynamics,coordinative structures,human brain,informationally coupled dynamical systems,metastability,phase transitions,self-organization},
  file = {/Users/AndyAlvarez/Google Drive/Zotero/Kelso(2014)The Dynamic Brain in Action.pdf}
}

