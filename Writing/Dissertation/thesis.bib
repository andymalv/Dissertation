@article{Ding2018,
  title = {Human-in-the-Loop Optimization of Hip Assistance with a Soft Exosuit during Walking},
  author = {Ding, Ye and Kim, Myunghee and Kuindersma, Scott and Walsh, Conor J.},
  year = {2018},
  journal = {Science Robotics},
  volume = {3},
  number = {15},
  pages = {eaar5438},
  issn = {2470-9476},
  doi = {10.1126/scirobotics.aar5438},
  abstract = {Wearable robotic devices have been shown to substantially reduce the energy expenditure of human walking. However, response variance between subjects for fixed control strategies can be high, leading to the hypothesis that individualized controllers could further improve walking economy. Recent studies on human-in-the-loop (HIL) control optimization have elucidated several practical challenges---in particular, long protocols and low signal-to-noise ratios. Here we show a method using Bayesian optimization---an algorithm well-suited to noisy signals with very limited data---to identify optimal peak and offset timing of hip extension assistance that minimizes the energy expenditure of walking with a textile-based wearable device. Optimal peak and offset timing were found over an average of 21.4 {\textpm} 1.0 min and reduced metabolic cost by 17.4 {\textpm} 3.2\% compared to walking without the device (mean {\textpm} s.e.m), which represented an improvement of more than 60\% on metabolic reduction compared to state-of-the-art devices only assisting hip extension. In addition, our results provide evidence for subject-specific metabolic distributions with respect to peak and offset timing, metabolic landscapes, lending support to the hypothesis that individualized control strategies can offer substantial benefits over fixed control strategies. These results also suggest that this method could have practical impact on improving the performance of wearable robotic devices.}
}

@inproceedings{Djuric2008,
