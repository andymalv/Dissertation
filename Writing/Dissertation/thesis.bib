@article{Ganesh2010,
  title = {Motor Memory and Local Minimization of Error and Effort, Not Global Optimization, Determine Motor Behavior.},
  author = {Ganesh, G and Haruno, M and Kawato, M and Burdet, E},
  year = {2010},
  journal = {Journal of neurophysiology},
  volume = {104},
  number = {1},
  eprint = {20484533},
  eprinttype = {pubmed},
  pages = {382--90},
  issn = {1522-1598},
  doi = {10.1152/jn.01058.2009},
  abstract = {Many real life tasks that require impedance control to minimize motion error are characterized by multiple solutions where the task can be performed either by co-contracting muscle groups, which requires a large effort, or, conversely, by relaxing muscles. However, human motor optimization studies have focused on tasks that are always satisfied by increasing impedance and that are characterized by a single error-effort optimum. To investigate motor optimization in the presence of multiple solutions and hence optima, we introduce a novel paradigm that enables us to let subjects repetitively (but inconspicuously) use different solutions and observe how exploration of multiple solutions affect their motor behavior. The results show that the behavior is largely influenced by motor memory with subjects tending to involuntarily repeat a recent suboptimal task-satisfying solution even after sufficient experience of the optimal solution. This suggests that the CNS does not optimize co-activation tasks globally but determines the motor behavior in a tradeoff of motor memory, error, and effort minimization.},
  isbn = {1522-1598 (Electronic){\textbackslash}n0022-3077 (Linking)},
  pmid = {20484533},
  keywords = {Adult,Algorithms,Calibration,Data Interpretation,Electromyography,Energy Metabolism,Energy Metabolism: physiology,Environment,Feedback,Female,Humans,Male,Memory,Memory: physiology,Movement,Movement: physiology,Muscle,Muscle Contraction,Muscle Contraction: physiology,Muscle Relaxation,Muscle Relaxation: physiology,Physiological,Physiological: physiology,Psychomotor Performance,Psychomotor Performance: physiology,Skeletal,Skeletal: physiology,Statistical,Young Adult}
}

