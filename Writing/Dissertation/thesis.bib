@article{Buckley2019,
  title = {Gait {{Asymmetry Post-Stroke}}: {{Determining Valid}} and {{Reliable Methods Using}} a {{Single Accelerometer Located}} on the {{Trunk}}},
  shorttitle = {Gait {{Asymmetry Post-Stroke}}},
  author = {Buckley, Christopher and {Mic{\'o}-Amigo}, M. Encarna and {Dunne-Willows}, Michael and Godfrey, Alan and Hickey, Aodh{\'a}n and Lord, Sue and Rochester, Lynn and Del Din, Silvia and Moore, Sarah A.},
  year = {2019},
  month = dec,
  journal = {Sensors},
  volume = {20},
  number = {1},
