@article{Morrison2016,
  title = {Assessing {{Multiple Sclerosis With Kinect}}: {{Designing Computer Vision Systems}} for {{Real-World Use}}},
  author = {Morrison, Cecily and Huckvale, Kit and Corish, Bob and Dorn, Jonas and Kontschieder, Peter and O'Hara, Kenton and Criminisi, Antonio and Sellen, Abigail and Dahlke, Frank and Kappos, Ludwig and Uitdehaag, Bernard and Burggraaff, Jessica and Kamm, Christian and Steinheimer, Saskia and D'Souza, Marcus},
  year = {2016},
  journal = {Human-Computer Interaction},
  volume = {31},
  number = {3-4},
  pages = {191--226},
  issn = {07370024},
  doi = {10.1080/07370024.2015.1093421},
  abstract = {The use of depth-sensing computer vision to capture bodily movement is{\textbackslash}nincreasingly being exploited in healthcare. Yet, there are few{\textbackslash}ndescriptions of how real-world practices influence the design of such{\textbackslash}napplications. To this end, we present the development and empirical{\textbackslash}nevaluation of ASSESS MS, a system to support the clinical assessment of{\textbackslash}nMultiple Sclerosis using Kinect. A key issue for developing{\textbackslash}nmachine-learning based systems is the need for standardized data on{\textbackslash}nwhich statistical inferences can be made. We demonstrate that there are{\textbackslash}nmany aspects of clinical practice that are at odds with the need to{\textbackslash}ncapture standardized data for a computer vision system. We offer three{\textbackslash}ndesign guidelines so address these: 1) Standardization is a{\textbackslash}nmulti-disciplinary issue and needs to be addressed early in the{\textbackslash}ndevelopment process; 2) Tools that provide a view into what the camera{\textbackslash}n``sees\{''\} can support the achievement of standardized data capture in{\textbackslash}nreal environments; 3) Tools to support standardized data capture should{\textbackslash}nmaintain the agency of human interaction. More broadly we show that when{\textbackslash}nconsidering every day contexts, the traditional focus on measurement{\textbackslash}naccuracy is only a small part of the effort needed to make a technology{\textbackslash}n``work\{''\} in practice.}
}

