@article{DeCarlo1992,
  title = {Traditional versus {{Accelerated Rehabilitation}} Following {{ACL Reconstruction}}: {{A One-Year Follow-Up}}},
  author = {De Carlo, M S and Shelbourne, K D and McCarroll, J R and Rettig, A C},
  year = {1992},
