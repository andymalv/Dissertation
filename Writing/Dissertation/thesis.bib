  title = {Estimation of Spatio-Temporal Parameters for Post-Stroke Hemiparetic Gait Using Inertial Sensors},
  author = {Yang, Shuozhi and Zhang, Jun-Tian and Novak, Alison C. and Brouwer, Brenda and Li, Qingguo},
