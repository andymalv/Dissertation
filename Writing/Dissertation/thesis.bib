@article{Kruger2008,
  title = {Prevalence of Transportation and Leisure Walking among {{U}}.{{S}}. Adults},
  author = {Kruger, Judy and Ham, Sandra A. and Berrigan, David and {Ballard-Barbash}, Rachel},
