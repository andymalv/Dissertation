@article{Marfeo2013,
  title = {Conceptual Foundation for Measures of Physical Function and Behavioral Health Function for Social Security Work Disability Evaluation},
  author = {Marfeo, Elizabeth E. and Haley, Stephen M. and Jette, Alan M. and Eisen, Susan V. and Ni, Pengsheng and Bogusz, Kara and Meterko, Mark and McDonough, Christine M. and Chan, Leighton and Brandt, Diane E. and Rasch, Elizabeth K.},
