  issn = {00014966},
  doi = {10.1097/WCO.0b013e328315a293.Understanding},
  abstract = {Feedback perturbation studies of speech acoustics have revealed a great deal about how speakers monitor and control their productions of segmental (e.g., formant frequencies) and non-segmental (e.g., pitch) linguistic elements. The majority of previous work, however, overlooks the role of acoustic feedback in consonant production and makes use of acoustic manipulations that effect either entire utterances or the entire acoustic signal, rather than more temporally and phonetically restricted alterations. This study, therefore, seeks to expand the feedback perturbation literature by examining perturbation of consonant acoustics that is applied in a time-restricted and phonetically specific manner. The spectral center of the alveopalatal fricative [{$\int$}] produced in vowel-fricative-vowel nonwords was incrementally raised until it reached the potential for [s]-like frequencies, but the characteristics of high-frequency energy outside the target fricative remained unaltered. An "offline," more widely accessible signal processing method was developed to perform this manipulation. The local feedback perturbation resulted in changes to speakers' fricative production that were more variable, idiosyncratic, and restricted than the compensation seen in more global acoustic manipulations reported in the literature. Implications and interpretations of the results, as well as future directions for research based on the findings, are discussed.},
  isbn = {1350-7540; 1350-7540},
  pmid = {18989103},
  keywords = {{\ss} 2008 wolters kluwer,628,633,Brain Mapping,Cerebellum,Cerebellum: physiology,Cerebral Cortex,Cerebral Cortex: physiology,curr opin neurol 21,Feedback,Feedback: physiology,health,Humans,lippincott williams,Magnetic Resonance Imaging,memory,Models,motor,Neurological,Psychomotor Performance,reaching,Speech,Speech: physiology,stroke,Verbal Behavior,walking,wilkins},
