}

@article{Hass2005,
  title = {Gait {{Initiation}} and {{Dynamic Balance Control}} in {{Parkinson}}'s {{Disease}}},
  author = {Hass, Chris J. and Waddell, Dwight E. and Fleming, Richard P. and Juncos, Jorge L. and Gregor, Robert J.},
