@article{Dejnabadi2005,
  title = {A New Approach to Accurate Measurement of Uniaxial Joint Angles Based on a Combination of Accelerometers and Gyroscopes},
  author = {Dejnabadi, H. and Jolles, B. M. and Aminian, K.},
  year = 2005,
